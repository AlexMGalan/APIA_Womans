<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:stretchColumns="1"
        android:background="@color/colorlightgrey">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:clipToPadding="false"
    android:background="@color/colorlightgrey">


    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/TextViewMSNPL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_gravity="center"
        android:text="Major Sponsors NPL"
        android:textSize="16sp"
        android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageViewCubic"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:src="@drawable/cubiclargenew"
            android:tag="https://cubic.com.au/"
            android:background="@drawable/back"
            android:onClick="openBrowser" />


        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clipToPadding="false"
            android:background="@color/colorlightgrey">

            <ImageView
                android:id="@+id/imageViewPara"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1"
                android:layout_marginLeft="20dp"
                android:src="@drawable/paramount"
                android:tag="https://www.paramountlawyers.com.au/"
                android:background="@drawable/back"
                android:onClick="openBrowser" />

            <ImageView
                android:id="@+id/imageViewSTE"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:src="@drawable/specialisttunnelexcavation"
                android:tag="http://bit.ly/2T2cUSy"
                android:background="@drawable/back"
                android:onClick="openBrowser" />

        </LinearLayout>


        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clipToPadding="false"
            android:background="@color/colorlightgrey">

            <ImageView
                android:id="@+id/imageViewBout1"
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1"
                android:layout_marginLeft="20dp"

                android:src="@drawable/boutiqueessential"
                android:tag="https://www.boutiqueresidential.com.au/"
                android:background="@drawable/back"
                android:onClick="openBrowser" />

            <ImageView
                android:id="@+id/imageViewIGS"
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:src="@drawable/igs"
                android:tag="https://www.igs.com.au/"
                android:background="@drawable/back"
                android:onClick="openBrowser" />

        </LinearLayout>

    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/TextViewMSGSAP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_gravity="center"
        android:text="Major Sponsors GSAP"
        android:textSize="16sp"
        android:textStyle="bold" />


        <ImageView
            android:id="@+id/imageViewSnadGSAP"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:src="@drawable/snaidero"
            android:tag="https://www.snaidero.com/"
            android:background="@drawable/back"
            android:onClick="openBrowser" />

        </LinearLayout>

    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/TextViewTeamSpon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="NPL Team Sponsors"
        android:textSize="16sp"
        android:textStyle="bold" />


    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/colorlightgrey">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/TextViewTeamSpon3"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="20dp"
            android:text="Under 17"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/imageViewCloud"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/it_on_cloud"
            android:tag="https://www.itoncloud.com/"
            android:background="@drawable/back"
            android:onClick="openBrowser" />

    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:clipToPadding="false"
        android:background="@color/colorlightgrey">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/TextViewTeamSpon4"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="30dp"
            android:text="Under 15"
            android:textSize="14sp" />



        <ImageView
            android:id="@+id/imageViewRazcon"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/paz"
            android:tag="http://www.ontimeconstruction.com.au/nsw/drummoyne_nsw.html"
            android:background="@drawable/back"
            android:onClick="openBrowser" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorlightgrey"
        android:clipToPadding="false"
        android:orientation="horizontal">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/TextViewTeamSpon5"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="30dp"
            android:text="Under 14"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/imageViewPain"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/pain"
            android:tag="https://painaway.com.au/"
            android:background="@drawable/back"
            android:onClick="openBrowser" />

    </LinearLayout>

    </TableLayout>
</ScrollView>
