<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:backgroundTint="@color/colorWhite">

    <TextView
        android:id="@+id/textViewSpace1"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorlightgrey" />

    <ImageView
        android:id="@+id/imageViewcubic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_gravity="center_horizontal"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:src="@mipmap/cubic" />

    <TextView
        android:id="@+id/textViewSpace2"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorlightgrey" />

    <ImageView
        android:id="@+id/imageViewGFDA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:onClick="openBrowserGFDA"
        android:src="@drawable/gfda"
        android:tag="https://www.girlsfootballacademyaustralia.com/" />

    <TextView
        android:id="@+id/textViewSpace3"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="20dp"
        android:background="@color/colorlightgrey" />

    <ImageView
        android:id="@+id/imageViewWests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_gravity="center_horizontal"
        android:layout_centerHorizontal="true"
        android:src="@drawable/wests_ashfield"
        android:tag="http://westsashfield.com.au/"
        android:onClick="openBrowserWests" />

    <TextView
        android:id="@+id/textViewSpace4"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="20dp"
        android:background="@color/colorlightgrey" />

    <ImageView
        android:id="@+id/imageacacia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:onClick="openBrowserWests"
        android:src="@drawable/acacia_sw"
        android:tag="http://acaciasoftware.com/" />
</LinearLayout>

